MATCH (n:A_TASK_TYPE_DECLARATION|A_FORMAL_TYPE_DECLARATION|A_SUBTYPE_DECLARATION|A_SUBTYPE_INDICATION|A_PRIVATE_TYPE_DECLARATION|AN_ORDINARY_TYPE_DECLARATION|A_PROTECTED_TYPE_DECLARATION|AN_INCOMPLETE_TYPE_DECLARATION|A_DISCRETE_SUBTYPE_INDICATION|A_MULTIPLY_OPERATOR|A_TAGGED_INCOMPLETE_TYPE_DECLARATION|AN_ALLOCATION_FROM_SUBTYPE|AN_ANONYMOUS_ACCESS_TO_PROCEDURE)<-[:IS_ENCLOSED_IN]-(def)
WHERE toUpper(n.enclosing_unit) IN $unitList
AND (n:AN_ABSTRACT_LIMITED_TRAIT OR n:AN_ABSTRACT_TRAIT OR n:AN_ABSTRACT_PRIVATE_TRAIT OR n:AN_ABSTRACT_LIMITED_PRIVATE_TRAIT OR def:AN_ABSTRACT_LIMITED_TRAIT OR def:AN_ABSTRACT_TRAIT OR def:AN_ABSTRACT_PRIVATE_TRAIT OR def:AN_ABSTRACT_LIMITED_PRIVATE_TRAIT)
return distinct n
 ORDER BY n.filename, n.line, n.column
